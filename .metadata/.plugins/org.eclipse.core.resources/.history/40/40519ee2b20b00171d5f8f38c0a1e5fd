package by.htp.homelibrary.launch;

import by.htp.homelibrary.domain.Book;
import by.htp.homelibrary.domain.Library;
import by.htp.homelibrary.domain.Magazine;
import by.htp.homelibrary.domain.PrintedEdition;

public class Main {
	public static void main(String[] args) { 
		Library library = new Library(); 
		Book book1 = new Book(15,"The Lie I've Lived");
		
		library.addNewEdition(book1); 
		library.addNewEdition(new Magazine(14, "WSJ"));
		library.addNewEdition(new Book(13, "Stephen King")); 
		library.addNewEdition(new Magazine(12, "Esquire")); 

		library.showLibrary(); 
		}
	PrintedEdition pred = new Book(13,"asd");
	
}
