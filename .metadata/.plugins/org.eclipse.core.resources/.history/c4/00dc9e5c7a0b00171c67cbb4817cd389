package by.htp.homelibrary;

public class Main {
	public static void main(String[] args) { 
		Library library = new Library(); 
		library.addNewEdition(new Book(10, "Potter")); 
		library.addNewEdition(new Journal(10, "Shakespear")); 
		library.addNewEdition(new Book(10, "Rodney")); 
		library.addNewEdition(new Journal(10, "Steward")); 

		library.showLibrary(); 
		}

}
