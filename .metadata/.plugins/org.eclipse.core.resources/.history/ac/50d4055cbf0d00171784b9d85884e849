package by.htp.hw2.main;

import java.io.InputStreamReader;
import java.util.Scanner;

public class Sort {
	public static void main(String [] Args){
		IntArray sort = new IntArray();
		
		Scanner sc = new Scanner(new InputStreamReader(System.in));
		
		
		sort.fill();
		sort.print();
		sort.ascendingSort();
		sort.print();
		sort.descendingSort();
		sort.print();
		sort.findMax();
		sort.findMin();
	}

}
